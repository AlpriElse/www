<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    {{#if css }}
      <link rel="stylesheet" href="{{ lookup webpack.assets css }}">
    {{else}}
      <link rel="stylesheet" href="{{ lookup webpack.assets 'site.css' }}">
    {{/if }}

    {{#if js }}
      <link rel="stylesheet" href="{{ lookup webpack.assets js }}">
    {{else}}
      <link rel="stylesheet" href="{{ lookup webpack.assets 'site.js' }}">
    {{/if }}
  </head>

  {{> navbar }}

